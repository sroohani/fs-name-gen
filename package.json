{
  "name": "@sroohani/fs-name-gen",
  "version": "0.0.1",
  "description": "Auto generates random file and folder names that look perfectly natural.",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc && microbundle --tsconfig tsconfig.json --no-sourcemap",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.15.29",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sroohani/fs-name-gen.git"
  },
  "keywords": [
    "generator",
    "name generator",
    "natural names",
    "zipf",
    "filename generator"
  ],
  "author": "Shahram Roohani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sroohani/fs-name-gen/issues"
  },
  "homepage": "https://sroohani.github.io"
}
